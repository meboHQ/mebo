{
  "name": "mebo",
  "version": "0.7.0",
  "description": "A framework focused to provide an unified interface across multiple domains",
  "scripts": {
    "build": "babel src --out-dir=dist --source-maps both",
    "build-dev": "babel src --watch --out-dir=dist --source-maps both",
    "prepublish": "npm run build",
    "start": "node dist",
    "docs": "npm install esdoc@0.5.2 --no-save && esdoc -c .esdoc.json && npm run docs-assets",
    "docs-assets": "cp -R data/ esdoc/data/",
    "lint": "eslint src/** test/**",
    "test": "nyc mocha --check-leaks --full-trace --recursive --exit",
    "test-debug": "mocha --inspect --trace-deprecation --debug-brk --watch --full-trace --recursive --compilers babel-register --exit",
    "test-cr": "madge ./src/ --circular",
    "test-ci": "npm run lint && npm run test-cr && npm test && codecov"
  },
  "author": "Paulo Nogueira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meboHQ/mebo/issues"
  },
  "homepage": "https://github.com/meboHQ/mebo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meboHQ/mebo.git"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "keywords": [
    "cli",
    "console",
    "web",
    "rest",
    "framework",
    "node",
    "es6",
    "es7"
  ],
  "dependencies": {
    "compare-versions": "^3.4.0",
    "debug": "^4.0.0",
    "ejs": "^2.6.1",
    "formidable": "^1.2.1",
    "ip": "^1.1.5",
    "js-typecheck": "^0.6.0",
    "lru-cache": "^4.1.3",
    "minimatch": "^3.0.4",
    "neodoc": "^2.0.2",
    "object-sizeof": "^1.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "codecov": "^3.1.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-spellcheck": "0.0.11",
    "express": "^4.16.3",
    "madge": "^3.2.0",
    "mocha": "^6.0.0",
    "nyc": "^14.0.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "request": "^2.88.0"
  },
  "optionalDependencies": {
    "xxhash": "^0.2.4"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
